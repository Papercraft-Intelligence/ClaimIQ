{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to .NET Process",
            "type": "coreclr",
            "request": "attach",
            "processName": "ClaimIq.Api"
        },
        {
            "name": "Debug ClaimIq.Api",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/ClaimIq.Api/bin/Debug/net8.0/ClaimIq.Api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ClaimIq.Api",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }
    ]
}